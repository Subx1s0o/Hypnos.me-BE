model review {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    name      String
    email     String
    comment   String
    rate      Int
    productId String   @db.ObjectId
    product   products @relation(fields: [productId], references: [id])
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
